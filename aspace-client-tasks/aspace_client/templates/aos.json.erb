{
  "title": <%= data['title'].to_json %>,
  "dates": [{
    "label": "creation",
    "date_type": "inclusive",
    "begin": <%= data['date'].to_json %><% unless data['end_date'].nil? %>,
    "end": <%= data['end_date'].to_json %><% end %>
  }],
  "level": "item",
  "resource": {"ref": <%= data['resource__ref'].to_json %>},
  "linked_agents": [{
    "ref": <%= data['people__refs'][0]['ref'].to_json %>,
    "role": <%= data['people__refs'][0]['role'].to_json %>
  }],
  "subjects": [{
    "ref": <%= data['subjects__refs'][0].to_json %>
  }]
}